{
  "name": "ai-skill-tree-assessment",
  "version": "1.0.0",
  "type": "module",
  "description": "2025年AI技能树评测问卷系统",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm install --include=optional --no-save || true",
    "build": "tsc && vite build",
    "build:ci": "npm ci && npm install --include=optional --no-save && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy:cloudflare": "npm run build && npx wrangler deploy"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.16",
    "three": "^0.160.0",
    "@react-three/fiber": "^8.15.11",
    "@react-three/drei": "^9.92.7",
    "recharts": "^2.10.3",
    "zustand": "^4.4.7",
    "jspdf": "^2.5.1",
    "html2canvas": "^1.4.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.43",
    "@types/three": "^0.160.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
